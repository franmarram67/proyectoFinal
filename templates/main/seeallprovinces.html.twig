{% extends 'base.html.twig' %}

{% block title %}Tournament Master{% endblock %}

{% block body %}

<h2>All Provinces</h2>
<div>
{% for province in provinces %}
    <div>
        <h3><a href="/seeprovince/{{province.id}}">{{province.name}}</a></h3>
        <img src="/img/{{province.image}}"/>
        <p>{{province.description}}</p>
    </div>
{% endfor %}
</div>

{% endblock %}
